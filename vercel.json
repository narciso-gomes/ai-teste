{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ],
  "build": {
    "env": {
      "NEXT_PUBLIC_GENERATE_PRISMA_CLIENT": "true"
    }
  }
}
